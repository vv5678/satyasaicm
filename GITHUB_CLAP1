#include<stdio.h>
int main()
{int a,b;
char operation;
scanf("%c",&operation);
scanf("%d %d",&a,&b);
switch (operation){
    case ('+'):
    printf("%d",a+b);
    break;
    case ('-'):
    printf("%d",a-b);
    break;
    case ('*'):
    printf("%d",a*b);
    break;
    case ('/'):
    printf("%d",a/b);
    break;
    }
return 0;
}
